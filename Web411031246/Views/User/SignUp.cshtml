
@model Web411031246.ViewModels.SignUPdata
@{
    ViewBag.Title = "SignUp";
}

<h2>SignUp</h2>

@using (Html.BeginForm("SignUp", "User", FormMethod.Post))
{
    <p> @Html.DisplayNameFor(m => m.Name):@Html.TextBoxFor(m => m.Name)@Html.ValidationMessageFor(m => m.Name)</p>
    <p> @Html.DisplayNameFor(m => m.Acconut): @Html.TextBoxFor(m => m.Acconut)@Html.ValidationMessageFor(m => m.Acconut)</p>
    <p> @Html.DisplayNameFor(m => m.Password):@Html.PasswordFor(m => m.Password)@Html.ValidationMessageFor(m => m.Password)</p>
    <input type="submit" value="注册" />
    <p> @Model.Message</p>

}



